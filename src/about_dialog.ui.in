<interface>
  <requires lib="libadwaita" version="1.0"/>
  <object class="AdwAboutWindow" id="about_window">
    <property name="modal">1</property>
    <property name="destroy-with-parent">1</property>
    <!-- Main page -->
    <property name="application-icon">io.github.unrud.RecentFilter</property>
    <property name="application-name" translatable="yes">Recent Filter</property>
    <property name="developer-name">Unrud</property>
    <property name="version">@VERSION@</property>
    <!-- What's new -->
    <property name="release-notes">@releasenotes@</property>
    <!-- Details -->
    <property name="website">https://github.com/unrud/recent-filter</property>
    <!-- Troubleshooting -->
    <property name="issue-url">https://github.com/unrud/recent-filter/issues</property>
    <!-- Credits and Acknowledgements -->
    <property name="developers">Unrud</property>
    <!-- Legal -->
    <property name="copyright">Copyright Â© 2023 Unrud</property>
    <property name="license-type">gpl-3-0</property>
  </object>
</interface>
